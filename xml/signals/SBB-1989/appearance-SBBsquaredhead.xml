    <appearance>
      <aspectname>Verde</aspectname>
      <show>green</show>
      <reference />
      <comment>Normal speed</comment>
      <imagelink type="vertical">../../../resources/icons/SBBSignals/Squaredhead/SBBNvssqgreen.gif</imagelink>
