{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "jmri-json-logixngicon-server-message",
    "type": "object",
    "description": "Data portion of message from JMRI to client for type \"logixngicon\"",
    "properties": {
        "name": {
            "type": "string",
            "description": "Always the string 'logixngicon'"
        },
        "data": {
            "type": "array",
            "description": "Data for the logixngicon",
            "items": {
                "identity": "integer",
                "description": "The identity of the 'logixngicon'"
            }
        }
    },
    "additionalProperties": false,
    "required": ["name", "data"]
}
